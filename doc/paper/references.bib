  author = {O'Reilly, Randall C.},
  month = nov,
  year = {1998},
  keywords = {bidirectional activation propagation,biological realism,distributed representations,error-driven learning,Hebbian learning,inhibitory competition,toread},
  pages = {455--462},
  file = {O'Reilly_1998_Six principles for biologically based computational models of cortical cognition.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/76WRRDXS/O'Reilly_1998_Six principles for biologically based computational models of cortical cognition.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/V7PNGVIQ/S1364661398012418.html:text/html}
}

