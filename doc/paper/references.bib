@article{sederberg2008,
  title = {A Context-Based Theory of Recency and Contiguity in Free Recall},
  volume = {115},
  copyright = {(c) 2012 APA, all rights reserved},
  issn = {1939-1471(Electronic);0033-295X(Print)},
  doi = {10.1037/a0013396},
  abstract = {The authors present a new model of free recall on the basis of M. W. Howard and M. J. Kahana's (see record 2002-17325-002) temporal context model and M. Usher and J. L. McClelland's (see record 2001-07628-003) leaky-accumulator decision model. In this model, contextual drift gives rise to both short-term and long-term recency effects, and contextual retrieval gives rise to short-term and long-term contiguity effects. Recall decisions are controlled by a race between competitive leaky accumulators. The model captures the dynamics of immediate, delayed, and continual distractor free recall, demonstrating that dissociations between short- and long-term recency can naturally arise from a model in which an internal contextual state is used as the sole cue for retrieval across time scales.},
  timestamp = {2015-01-27T20:29:27Z},
  number = {4},
  journal = {Psychological Review},
  author = {Sederberg, Per B. and Howard, Marc W. and Kahana, Michael J.},
  year = {2008},
  keywords = {*Free Recall,*Models,*Recency Effect,Cues,Distraction},
  pages = {893--912},
  file = {Sederberg et al_2008_A context-based theory of recency and contiguity in free recall.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/UH55CU49/Sederberg et al_2008_A context-based theory of recency and contiguity in free recall.pdf:application/pdf}
}

@article{usher2001,
  title = {The Time Course of Perceptual Choice: {{The}} Leaky, Competing Accumulator Model},
  volume = {108},
  copyright = {(c) 2012 APA, all rights reserved},
  issn = {1939-1471(Electronic);0033-295X(Print)},
  shorttitle = {The Time Course of Perceptual Choice},
  doi = {10.1037/0033-295X.108.3.550},
  abstract = {The time course of perceptual choice is discussed in a model of gradual, leaky, stochastic, and competitive information accumulation in nonlinear decision units. Special cases of the model match a classical diffusion process, but leakage and competition work together to address several challenges to existing diffusion, random walk, and accumulator models. The model accounts for data from choice tasks using both time-controlled (e.g., response signal) and standard reaction time paradigms and its adequacy compares favorably with other approaches. A new paradigm that controls the time of arrival of information supporting different choice alternatives provides further support. The model captures choice behavior regardless of the number of alternatives, accounting for the log-linear relation between reaction time and number of alternatives (Hick's law) and explains a complex pattern of visual and contextual priming in visual word identification.},
  timestamp = {2015-01-29T21:07:26Z},
  number = {3},
  journal = {Psychological Review},
  author = {Usher, Marius and McClelland, James L.},
  year = {2001},
  keywords = {_tablet,*Choice Behavior,*Cognitive Processes,Models},
  pages = {550--592},
  file = {Usher_McClelland_2001_The time course of perceptual choice.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/2AVPZ28K/Usher_McClelland_2001_The time course of perceptual choice.pdf:application/pdf}
}

@article{churchland2008,
  title = {Decision-Making with Multiple Alternatives},
  volume = {11},
  copyright = {\textcopyright{} 2008 Nature Publishing Group},
  issn = {1097-6256},
  doi = {10.1038/nn.2123},
  abstract = {Simple perceptual tasks have laid the groundwork for understanding the neurobiology of decision-making. Here, we examined this foundation to explain how decision-making circuitry adjusts in the face of a more difficult task. We measured behavioral and physiological responses of monkeys on a two- and four-choice direction-discrimination decision task. For both tasks, firing rates in the lateral intraparietal area appeared to reflect the accumulation of evidence for or against each choice. Evidence accumulation began at a lower firing rate for the four-choice task, but reached a common level by the end of the decision process. The larger excursion suggests that the subjects required more evidence before making a choice. Furthermore, on both tasks, we observed a time-dependent rise in firing rates that may impose a deadline for deciding. These physiological observations constitute an effective strategy for handling increased task difficulty. The differences appear to explain subjects' accuracy and reaction times.},
  language = {en},
  timestamp = {2016-12-05T20:08:58Z},
  number = {6},
  urldate = {2016-12-05},
  journal = {Nature Neuroscience},
  author = {Churchland, Anne K. and Kiani, Roozbeh and Shadlen, Michael N.},
  month = jun,
  year = {2008},
  keywords = {_tablet},
  pages = {693--702},
  file = {Churchland et al_2008_Decision-making with multiple alternatives.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/3EDWQCJA/Churchland et al_2008_Decision-making with multiple alternatives.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/JJSQKMF6/nn.2123.html:text/html}
}

@article{furman2008,
  title = {Similarity Effect and Optimal Control of Multiple-Choice Decision Making},
  volume = {60},
  issn = {1097-4199},
  doi = {10.1016/j.neuron.2008.12.003},
  abstract = {Decision making with several choice options is central to cognition. To elucidate the neural mechanisms of such decisions, we investigated a recurrent cortical circuit model in which fluctuating spiking neural dynamics underlie trial-by-trial stochastic decisions. The model encodes a continuous analog stimulus feature and is thus applicable to multiple-choice decisions. Importantly, the continuous network captures similarity between alternatives and possible overlaps in their neural representation. Model simulations accounted for behavioral as well as single-unit neurophysiological data from a recent monkey experiment and revealed testable predictions about the patterns of error rate as a function of the similarity between the correct and actual choices. We also found that the similarity and number of options affect speed and accuracy of responses. A mechanism is proposed for flexible control of speed-accuracy tradeoff, based on a simple top-down signal to the decision circuit that may vary nonmonotonically with the number of choice alternatives.},
  language = {eng},
  timestamp = {2016-12-06T15:48:12Z},
  number = {6},
  journal = {Neuron},
  author = {Furman, Moran and Wang, Xiao-Jing},
  month = dec,
  year = {2008},
  keywords = {_tablet,Action Potentials,alpha-Amino-3-hydroxy-5-methyl-4-isoxazolepropionic Acid,Animals,Cerebral Cortex,Computer simulation,Decision making,Discrimination (Psychology),Excitatory Amino Acid Agonists,Models; Neurological,motion perception,Nerve Net,Neural Networks (Computer),Neuronal Plasticity,Neurons,Nonlinear Dynamics,reaction time,Synapses,Time Factors},
  pages = {1153--1168},
  file = {Furman_Wang_2008_Similarity effect and optimal control of multiple-choice decision making.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/34VFJFF3/Furman_Wang_2008_Similarity effect and optimal control of multiple-choice decision making.pdf:application/pdf},
  pmid = {19109918},
  pmcid = {PMC2633638}
}

@article{oreilly1998,
  title = {Six Principles for Biologically Based Computational Models of Cortical Cognition},
  volume = {2},
  issn = {1364-6613},
  doi = {10.1016/S1364-6613(98)01241-8},
  abstract = {This review describes and motivates six principles for computational cognitive neuroscience models: biological realism, distributed representations, inhibitory competition, bidirectional activation propagation, error-driven task learning, and Hebbian model learning. Although these principles are supported by a number of cognitive, computational and biological motivations, the prototypical neural-network model (a feedforward back-propagation network) incorporates only two of them, and no widely used model incorporates all of them. It is argued here that these principles should be integrated into a coherent overall framework, and some potential synergies and conflicts in doing so are discussed.},
  timestamp = {2017-01-16T14:52:41Z},
  number = {11},
